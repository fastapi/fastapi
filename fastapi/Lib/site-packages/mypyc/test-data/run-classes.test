class C:
    lurr: ClassVar[int] = 9
    @staticmethod
    def foo(x: int) -> int:
        return 10 + x
    @classmethod
    def bar(cls, x: int) -> int:
        return cls.lurr + x
    @staticmethod
    def baz(x: int, y: int = 10) -> int:
        return y - x
    @classmethod
    def quux(cls, x: int, y: int = 10) -> int:
        return y - x
    @classmethod
    def call_other(cls, x: int) -> int:
        return cls.quux(x, 3)

